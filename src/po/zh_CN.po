#: ../src/cli/cli.c:56
msgid ""
"& [-vsp] -L[PREFIX] [DUMP_DIR]\n"
"   or: & [-vsp] -e EVENT DUMP_DIR\n"
"   or: & [-vsp] -a[y] DUMP_DIR\n"
"   or: & [-vsp] -c[y] DUMP_DIR\n"
"   or: & [-vsp] -r[y|o|d] DUMP_DIR"
msgstr "& [-vsp] -L[前缀] [转储目录]\n   or: & [-vsp] -e 事件 转储目录\n   or: & [-vsp] -a[y] 转储目录\n   or: & [-vsp] -c[y] 转储目录\n   or: & [-vsp] -r[y|o|d] 转储目录"

